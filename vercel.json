{
    "version": 2,
    "framework": null,
    "buildCommand": "",
    "installCommand": "",
    "outputDirectory": "public",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=86400, immutable"
                }
            ]
        }
    ]
}